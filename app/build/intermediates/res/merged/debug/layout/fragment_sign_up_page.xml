<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ScrollView1_3b"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_skin7" >

    <LinearLayout
        android:id="@+id/LinearLayout1_3b"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        tools:context="com.foodline.controller.SignUpPage$PlaceholderFragment" >

        <TextView
            android:id="@+id/textView1_3b"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="92dp"
            android:text="@string/title_sign_up_page_3"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <TableLayout
            android:id="@+id/TableLayout1_3b"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/textView2_3b"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/email_textview_3"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <EditText
                android:id="@+id/editText1_3b"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:hint="@string/email_hint_edit_text_3"
                android:inputType="textEmailAddress" />

            <TextView
                android:id="@+id/textView3_3b"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="14dp"
                android:text="@string/first_name_text_view_3"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <EditText
                android:id="@+id/editText2_3b"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="14dp"
                android:ems="10"
                android:hint="@string/first_name_hint_edit_text_3" />

            <TextView
                android:id="@+id/textView4_3b"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/last_name_text_view_3"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <EditText
                android:id="@+id/editText3_3b"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:hint="@string/last_name_hint_edit_text_3" />

            <TextView
                android:id="@+id/textView5_3b"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/password_text_view_3"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <EditText
                android:id="@+id/editText4_3b"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPassword" />

            <TextView
                android:id="@+id/textView6_3b"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/phone_text_view_3"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <EditText
                android:id="@+id/editText5_3b"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:hint="@string/phone_hint_edit_text_3"
                android:inputType="phone" />
        </TableLayout>

        <CheckBox
            android:id="@+id/checkBox1_3b"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/agreement_text_view_3" />

        <Button
            android:id="@+id/button1_3b"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="onSubmitButtonClick"
            android:text="@string/submit_button_3" />

    </LinearLayout>

</ScrollView>